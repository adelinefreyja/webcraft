<body>
  <div id="preloader"></div>

  <section id="hero" style="background: url('{{ design.getHeaderImg }}') top center fixed">
    <div class="hero-container">
      <div class="wow fadeIn">
        <div class="hero-logo">
          <img class="" src="{{ logo.optionvalue }}" alt="">
        </div>

        <h1>{{ siteinfo.optionname }}</h1>
        <h2>{{ siteinfo.optionvalue }}</h2>

        <div class="actions">
          <a href="#about" class="btn-get-started">Get Stated</a>
          <a href="#services" class="btn-services">Our Services</a>
        </div>
      </div>
    </div>
  </section>

  <header id="header">
    <div class="container">

      <div id="logo" class="pull-left">
        <a href="#hero"><img src="{{ logo.optionvalue }}" alt="" title="" /></img></a>
      </div>

      <nav id="nav-menu-container">
        <ul class="nav-menu">
          {% set compt = [] %}
              {% for page in menu %}
                 {% for i in pages|sort %}
                     {% if page.pageName == i.pageName and i.pageStatus == "afficher" %}
                        <li><a href="#{{ page.pagename }}">{{ page.pagename }}</a></li>
                    {% endif %}
                {% endfor %}
              {% endfor %}
          {% if contact.sitetype == 3 %}
            <li><a href="#contact">Contact</a></li>
          {% endif %}
        </ul>
      </nav>
    </div>
  </header>

{# {% for page in pages %}
