<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}{{ Pages.pageTitle }}{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Source+Code+Pro" rel="stylesheet">
        	  <link rel="stylesheet" type="text/css" href="{{ asset('css/fa-svg-with-js.css') }}">
        	  <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('css/fonts.css') }}" />
        {% endblock %}
        {% block specificcss %}
        {% endblock %}
    </head>
    <body>
    	<header>
    	    <nav class="brown">
    		  <div class="nav-wrapper">


    		{% block nav %}
                {% block logo %}{% endblock %}
            {% endblock %}

    		</div>
    	</nav>

    	</header>

        {% block content %}

        {% endblock %}

        <footer>
        {% block footer %}{% endblock %}
        </footer>

        {% block javascripts %}
        	<script type="text/javascript" src="{{ asset('js/fontawesome-all.min.js') }}"></script>
        	<script type="text/javascript" src="{{ asset('js/jquery-3.2.1.min.js') }}"></script>
          <script type="text/javascript" src="{{ asset('js/jquery.webui-popover.js') }}"></script>
        	<script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}"></script>
        {% endblock %}

        {% block specificjs %}
        {% endblock %}
    </body>
</html>
